{
	"title": "Math Notebook",
	"author": "MatrixXT",
	"language": "zh-hans",
    "plugins": [
    	"mathjax",
    	"anchor-navigation-ex",
    	"-sharing"
    ],
    "pluginsConfig": {
    	"theme-default": {
            "showLevel": true
